{
  "name": "@develerik/har-viewer",
  "version": "0.7.0",
  "private": true,
  "description": "HTTP Archive Viewer",
  "keywords": [
    "har",
    "http",
    "viewer"
  ],
  "homepage": "https://github.com/develerik/har-viewer",
  "bugs": "https://github.com/develerik/har-viewer/issues",
  "repository": "git@github.com:develerik/har-viewer.git",
  "license": "ISC",
  "author": {
    "name": "Erik Bender",
    "email": "erik.bender@develerik.dev",
    "url": "https://develerik.dev"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "deploy": "cross-env DEPLOY=1 node scripts/gh-pages-deploy.js",
    "lint": "eslint --ext js,vue .",
    "serve": "webpack serve",
    "start": "http-server dist --no-dotfiles --gzip --brotli -c86400"
  },
  "dependencies": {
    "@mdi/svg": "7.2.96",
    "dialog-polyfill": "0.5.6",
    "vue": "3.2.47"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "3.2.47",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.7.3",
    "cssnano": "6.0.0",
    "eslint": "8.38.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-import-resolver-node": "0.3.7",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsdoc": "41.1.1",
    "eslint-plugin-vue": "9.10.0",
    "execa": "7.1.1",
    "favicons": "7.1.2",
    "favicons-webpack-plugin": "6.0.0",
    "html-webpack-plugin": "5.5.0",
    "http-server": "14.1.1",
    "mini-css-extract-plugin": "2.7.5",
    "postcss": "8.4.21",
    "postcss-import": "15.1.0",
    "postcss-loader": "7.2.4",
    "postcss-preset-env": "8.3.1",
    "sass": "1.62.0",
    "sass-loader": "13.2.2",
    "style-loader": "3.3.2",
    "terser-webpack-plugin": "5.3.7",
    "vue-loader": "17.0.1",
    "webpack": "5.79.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.13.2",
    "workbox-webpack-plugin": "6.5.4"
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "pnpm": {
    "overrides": {
      "rollup-plugin-terser": "npm:@rollup/plugin-terser@0.4.1",
      "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@1.4.15"
    }
  }
}
